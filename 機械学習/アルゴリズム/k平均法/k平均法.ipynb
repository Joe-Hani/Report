      },
      "source": [
        "from sklearn.cluster import KMeans\n",
        "kmeans = KMeans(n_clusters=2) #k=2として、k平均法を適用\n",
        "y_pred = kmeans.fit(X).predict(X) #Xをクラスタリングする\n",
        "print(y_pred)"
      ],
      "execution_count": 2,
